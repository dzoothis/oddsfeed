# Laravel Backend
# =============================================================================

# Laravel specific
/vendor/
/node_modules/
/npm-debug.log*
/yarn-debug.log*
/yarn-error.log*

# Laravel storage and logs
/storage/app/*
!/storage/app/.gitkeep
/storage/framework/cache/*
!/storage/framework/cache/.gitkeep
/storage/framework/sessions/*
!/storage/framework/sessions/.gitkeep
/storage/framework/views/*
!/storage/framework/views/.gitkeep
/storage/logs/*
!/storage/logs/.gitkeep
!/storage/framework/.gitkeep

# Laravel public files
/public/build
/public/hot
/public/storage
/public/uploads
/public/assets/
/public/index.html

# Laravel environment files
.env
.env.backup
.env.local
.env.production
.env.staging
.env.testing
.env.example

# Laravel specific cache and temp files
/bootstrap/cache/*
!/bootstrap/cache/.gitkeep
.phpunit.result.cache
.phpunit.cache/

# Laravel compiled files
/bootstrap/compiled.php

# Laravel Sail
/sail
/docker-compose.override.yml

# Laravel Homestead
Homestead.json
Homestead.yaml

# Laravel Vapor
/vapor

# Laravel authentication
/auth.json

